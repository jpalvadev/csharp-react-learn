    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>89903b4b-9b5d-4df3-8c62-9b03f096c185</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="16.0.0" />
